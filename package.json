{
  "name": "treasure-hunter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,html,css,json}": [
      "prettier --write --ignore-unknown"
    ],
    "*.ts": [
      "tsc-files --noEmit -p tsconfig.husky.json"
    ]
  },
  "devDependencies": {
    "@types/animejs": "^3.1.4",
    "@types/howler": "^2.2.7",
    "@types/node": "^17.0.25",
    "autoprefixer": "^10.4.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "postcss": "^8.4.12",
    "prettier": "2.6.2",
    "tailwindcss": "^3.0.24",
    "tsc-files": "^1.1.3",
    "typescript": "^4.5.4",
    "vite": "^2.9.0"
  },
  "dependencies": {
    "animejs": "^3.2.1",
    "howler": "^2.2.3"
  }
}
